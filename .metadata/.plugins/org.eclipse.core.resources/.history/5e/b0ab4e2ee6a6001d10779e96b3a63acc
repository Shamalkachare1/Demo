package Pages.StepDefinition;

public class anagram
{
	public static void main(String a[])
	{
		String a1= "anagram";
		String b= "gramana";
		int count=0;
		char[] arr= a1.toCharArray();
		char[] arr2=b.toCharArray();
		
		for(int i=a1.length()-1;i>=0;i--)
			
			for(int j=b.length()-1; j>=0;j--)
			{
				if(arr[i]==arr2[j])
				{
					count= count+1;
				}
			}

	}
}